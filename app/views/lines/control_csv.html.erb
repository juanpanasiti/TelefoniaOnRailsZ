<h2>Control de líneas</h2>
<div class="tab-content">
  <div class="tab-pane active" id="reduced_csv_tab" role="tabpanel">
    <table class="table table-striped table-hover display responsive no-wrap" id="control_table">
      <thead>
        <tr>
          <th>
            N°
          </th>
          <th> <%= @table[0][0] %> </th>
          <th> <%= @table[0][1] %> </th>
          <th> <%= @table[0][2] %> </th>
          <th> <%= @table[0][3] %> </th>
          <th> <%= @table[0][4] %> </th>
          <th> <%= @table[0][6] %> </th>
          <th> <%= @table[0][5] %> </th>
          <th> <%= @table[0][7] %> </th>
        </tr>
      </thead>
      <tbody>
        <% n = 1 %>
        <% @table[1].each do |q| %>
        <tr>
          <td>
            <%= n %>
          </td>
          <td> <%= q[0] %> </td>
          <td> <%= q[1] %> </td>
          <td> <%= q[2] %> </td>
          <td> <%= q[3] %> </td>
          <td> <%= q[4] %> </td>
          <td> <%= q[6] %> </td>
          <td> <%= q[5] %> </td>
          <td> <%= q[7] %> </td>
        </tr>
        <% n = n + 1 %>
        <% end %>
      </tbody>
    </table>
    <script>
    $(document).ready(function() {
      $('#control_table').DataTable({
        responsive: true
      });
    });
    </script>
