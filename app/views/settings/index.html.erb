<%= content_for :tabs do %>
  <!-- tabs -->
    <div class="mdl-layout__tab-bar mdl-js-ripple-effect">
      <a href="#scroll-tab-0" class="mdl-layout__tab is-active">Selectores</a>
      <a href="#scroll-tab-1" class="mdl-layout__tab">Oficinas</a>
      <a href="#scroll-tab-2" class="mdl-layout__tab"> Equipos (Modelos)</a>
      <a href="#scroll-tab-3" class="mdl-layout__tab">Facturación</a>
    </div>

<% end %>

<!-- Main content -->
<section class="mdl-layout__tab-panel is-active" id="scroll-tab-0">
  <div class="page-content">
    <%= render "selector_index", selector_settings: @selector_settings %>
  </div>
</section>
<section class="mdl-layout__tab-panel" id="scroll-tab-1">
  <div class="page-content">
    <%= render "offices_index", offices: @offices %>
  </div>
</section>
<section class="mdl-layout__tab-panel" id="scroll-tab-2">
  <div class="page-content">Internos</div>
</section>
<section class="mdl-layout__tab-panel" id="scroll-tab-3">
  <div class="page-content">
    facturación
  </div>
</section>
